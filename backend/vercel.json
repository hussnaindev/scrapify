{
    "version": 2,
    "builds": [
        {
            "src": "backend/package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/api/$1"
        }
    ],
    "buildCommand": "cd backend && npm run build",
    "installCommand": "cd backend && npm install"
}